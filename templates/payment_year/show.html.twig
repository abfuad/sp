    <h1>PaymentYear</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ payment_year.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ payment_year.name }}</td>
            </tr>
            <tr>
