<form method="post" action="{{ path('app_special_income_delete', {'id': data.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ data.id) }}">
    <button class="btn btn-danger btn-sm"><i class="fa fa-trash"></i></button>
</form>
