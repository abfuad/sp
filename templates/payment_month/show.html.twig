{% extends 'base.html.twig' %}

{% block title %}PaymentMonth{% endblock %}

{% block body %}
    <h1>PaymentMonth</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ payment_month.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ payment_month.name }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ payment_month.code }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ payment_month.description }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ payment_month.createdAt ? payment_month.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ payment_month.updatedAt ? payment_month.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_payment_month_index') }}">back to list</a>

    <a href="{{ path('app_payment_month_edit', {'id': payment_month.id}) }}">edit</a>

    {{ include('payment_month/_delete_form.html.twig') }}
{% endblock %}
