<form method="post" class="d-inline" action="{{ path('app_penality_fee_delete', {'id': data.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ data.id) }}">
    <button class="btn btn-sm btn-danger" title="delete"><i class="fa fa-trash"></i></button>
</form>
