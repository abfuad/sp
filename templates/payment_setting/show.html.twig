{% extends 'base.html.twig' %}

{% block title %}PaymentSetting{% endblock %}

{% block body %}
    <h1>PaymentSetting</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ payment_setting.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ payment_setting.name }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ payment_setting.code }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ payment_setting.description }}</td>
            </tr>
            <tr>
                <th>Amount</th>
                <td>{{ payment_setting.amount }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ payment_setting.createdAt ? payment_setting.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ payment_setting.updatedAt ? payment_setting.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_payment_setting_index') }}">back to list</a>

    <a href="{{ path('app_payment_setting_edit', {'id': payment_setting.id}) }}">edit</a>

    {{ include('payment_setting/_delete_form.html.twig') }}
{% endblock %}
