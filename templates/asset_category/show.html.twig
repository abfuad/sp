{% extends 'base.html.twig' %}

{% block title %}AssetCategory{% endblock %}

{% block body %}
    <h1>AssetCategory</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ asset_category.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ asset_category.name }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ asset_category.code }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ asset_category.description }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ asset_category.createdAt ? asset_category.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ asset_category.updatedAt ? asset_category.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_asset_category_index') }}">back to list</a>

    <a href="{{ path('app_asset_category_edit', {'id': asset_category.id}) }}">edit</a>

    {{ include('asset_category/_delete_form.html.twig') }}
